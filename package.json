    "react-router-dom": "^5.1.2",
    "react-test-renderer": "^16.9.0",
    "remark-cli": "^6.0.1",
    "remark-frontmatter": "^1.3.2",
    "remark-lint": "^6.0.5",
    "remark-parse": "^6.0.3",
    "remark-preset-lint-recommended": "^3.0.3",
    "remark-stringify": "^6.0.4",
    "remark-yaml-config": "^4.1.0",
    "rimraf": "^2.6.3",
    "sinon": "^7.3.2",
    "style-loader": "^0.23.1",
    "styled-components": "^4.3.2",
    "stylelint": "^10.1.0",
    "stylelint-config-prettier": "^5.2.0",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-standard": "^18.3.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-declaration-block-no-ignored-properties": "^2.1.0",
    "stylelint-order": "^3.0.0",
    "stylelint-processor-styled-components": "^1.8.0",
    "typescript": "3.5.3",
    "webpack-bundle-analyzer": "^3.8.0",
    "xhr-mock": "^2.5.0"
  },
  "scripts": {
    "fdev": "baozheng dev",
    "build": "lerna run build --stream",
    "postinstall": "yarn all",
    "all": "lerna bootstrap && lerna exec yarn --stream",
    "selfcheck": "yarn type-check && yarn test",
    "type-check": "lerna run build:types --stream",
    "test": "BABEL_ENV=test jest --detectOpenHandles --forceExit --verbose --coverage",
    "test:last": "BABEL_ENV=test jest --detectOpenHandles --forceExit --verbose --coverage --changedSince origin/master",
    "test:clear": "BABEL_ENV=test jest --clearCache",
    "test:debug": "BABEL_ENV=test node --inspect-brk node_modules/.bin/jest --runInBand",
    "test:watch": "BABEL_ENV=test jest --watch --verbose --coverage",
    "publish-doc": "yarn test && baozheng build -a && node ./script/index.js",
    "lerna:publish": "yarn build && lerna publish --yes --no-commit-ho oks"
  },
  "pre-commit": [
    "lint-staged"
  ],
  "lint-staged": {
    "linters": {
      "packages/**/*.{tsx,ts}": [
        "stylelint"
      ],
      "**/*.{js,jsx,tsx,ts}": [
        "eslint --ext .js,.jsx,.ts,.tsx"
      ],
      "**/*.{md,mdx}": [
        "remark"
      ],
      "**/*.{js,jsx,tsx,ts,less,md,json}": [
        "prettier --write",
        "git add"
      ]
    }
  },
  "sideEffects": [
    "api/api.ts"
  ],
  "engines": {
    "node": ">=8.0.0"
  },
  "workspaces": [
    "packages/*"
  ]
}
